{
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "parser": "babel-eslint",
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "error",
        "react/prop-types": "warn",
        "react/jsx-filename-extension": "off",
        "react/prefer-stateless-function": "off"
    },
    "plugins": ["prettier", "react"],
    "globals": {
        "localStorage": true,
        "Promise": true,
        "config": true
    }
}
